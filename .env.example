# Required: Google Gemini API Key for AI research (default)
GOOGLE_API_KEY=your_google_api_key_here

# Optional: OpenAI API Key (alternative to Google)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Anthropic API Key (alternative to Google/OpenAI)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: LLM Provider (google, openai, or anthropic)
LLM_PROVIDER=google

# Optional: Model Configuration
GOOGLE_MODEL=gemini-2.5-flash
OPENAI_MODEL=gpt-4
ANTHROPIC_MODEL=claude-3-sonnet-20240229

# Optional: Data storage path
DATA_PATH=./data